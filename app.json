{
  "expo": {
    "name": "Feed My People",
    "slug": "feed-my-people",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "permissions": [
      "location"
    ],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Feed My People to access your location to show directions to our office."
        }
      ]
    ],
    "splash": {
      "image": "./app/assets/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.kg14.feedmypeople"
    },
    "web": {
      "favicon": "./app/assets/logo1.png"
    },
    "extra": {
      "eas": {
        "projectId": "79e0c81d-b246-4dfa-a462-1965e2d856aa"
      }
    }
  }
}
